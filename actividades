#ejercicio 4
n = int(input())
for x in range ((n-1),(n*2)):
    if (n+1)>=x:
        print(x)
    if (x == ((n*2)-1))
        print(x)
#ejercicio 5
frase= input ("frase ")
print("las vocales de la frase son: ")
for r in "a,e,i,o,u":
    if r in frase:
        print(r)
